[include]
node_modules/

[libs]
flow/lib
flow-typed/npm

[ignore]
dist/.*
.*babel(?!-polyfill).*
.*flow-typed.*
.*node_modules/.*js.flow
.*node_modules/apollo-client/apollo.umd.js.flow
.*node_modules/react-apollo/react-apollo.umd.js.flow
.*node_modules/graphql/execution/execute.js.flow
.*node_modules/graphql/graphql.js.flow
.*node_modules/graphql/subscription/subscribe.js.flow
.*node_modules/webpack-cli/.nyc_output/.*.json
[options]
esproposal.class_instance_fields=enable
suppress_comment=\\(.\\|\n\\)*\\$FlowIgnoreError
